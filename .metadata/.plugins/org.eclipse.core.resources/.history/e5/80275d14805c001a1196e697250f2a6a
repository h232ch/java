package Chapter3;

public class OperatorEx7 {
	
	public static void main(String[] args) {
		
		int s;
		int i=1;
		int j=7;
		while (i<=j) {
			System.out.print("/ : "+j/i);
			i++;
		}
		System.out.println("");
		i=1;
		while (i<=j) {
			System.out.print("% : "+7%i);
			i++;
		}
		
		
		for(int k=1; k<j; i++) {
			
			if(k==2) {
				s=3;
			}
			
		}
		
		
		
		
		
	}

}
