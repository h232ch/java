package Chapter3;

public class OperatorEx5 {
	
	public static void main(String[] args) {
		
		
		//Last Test

		int i=0;
		int j=7;
			
		switch (i) {
			case 0:
				for(int k=1; k<8; k++) {
					if(k==4) {
						System.out.print('*');
					}else if(k==7){
						System.out.println(' ');
					}else {
						System.out.print(' ');
					}
				}
			case 1:
				for(int k=1; k<7; k++) {
					if(k==3 || k==4 || k==5) {
						System.out.print('*');
					}else if(k==7){
						System.out.println(' ');
					}else {
						System.out.print(' ');
					}
				}
			case 2:
				for(int k=1; k<7; k++) {
					if(k==2|| k==3 || k==4 || k==5||k==6) {
						System.out.print('*');
					}else if(k==7){
						System.out.println(' ');
					}else {
						System.out.print(' ');
					}
				}
			case 3:
				for(int k=1; k<7; k++) {
					
					if(k!=7) {
						System.out.print('*');
					}else{
						System.out.println('*');
					}
				}
				
			case 4:
				for(int k=1; k<7; k++) {
					if(k==2|| k==3 || k==4 || k==5||k==6) {
						System.out.print('*');
					}else if(k==7){
						System.out.println(' ');
					}else {
						System.out.print(' ');
					}
				}
			case 5:
				for(int k=1; k<7; k++) {
					if(k==3 || k==4 || k==5) {
						System.out.print('*');
					}else if(k==7){
						System.out.println(' ');
					}else {
						System.out.print(' ');
					}
				}
			case 6:
				for(int k=1; k<7; k++) {
					if(k==3) {
						System.out.print('*');
					}else if(k==6){
						System.out.println(' ');
					}else {
						System.out.print(' ');
					}
				}
			
		}
				
	}

}
