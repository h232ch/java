		int lineCount = scanner.nextInt();
		int spaceCount = lineCount/2 +1;
		int starCount=1;
		
		System.out.println(spaceCount);
		
		while(starCount<=lineCount) {
			while() {
				
			}
		}
		
		
	}
}
