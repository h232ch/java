package Chapter3;

public class OperatorEx7 {
	
	public static void main(String[] args) {
		
		int s;
		int i=1;
		int j=7;
		while (i<=j) {
			System.out.print("/ : "+j/i);
			i++;
		}
		System.out.println("");
		i=1;
		while (i<=j) {
			System.out.print("% : "+j%i);
			i++;
		}
		
		
		for(int k=1; k<j; i++) {
			
			if(k==3) {
				s=3;
			}else {
				s=j%k;
			}
			
			switch(s) {
			case(0):
				if(k==7) {
					System.out.println(' ');
				}else if(k<3 && k<5) {
					
				}
			}
			
		}
		
		
		
		
		
	}

}
