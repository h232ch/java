package Chapter3;

public class OperatorEx5 {
	
	public static void main(String[] args) {
		
		
		//Last Test

		int i;
		int j=7;
		
		for (i=0; i<=j; i++) {
			
			switch (i) {
				case 0:
					for(int k=1; k<7; k++) {
						if(k==3) {
							System.out.print('*');
						}else {
							System.out.print(' ');
						}
					}
				case 1:
					for(int k=1; k<7; k++) {
						
						if(k==2 || k==3 || k==4) {
							System.out.print('*');
						}else {
							System.out.print(' ');
						}
					}
				case 2:
					for(int k=1; k<7; k++) {
						System.out.print('*');
					}
				case 3:
					for(int k=1; k<7; k++) {
						
						if(k==2 || k==3 || k==4) {
							System.out.print('*');
						}else {
							System.out.print(' ');
						}
					}
				case 4:
					for(int k=1; k<7; k++) {
						System.out.print(' ');
						if(k==3) {
							System.out.print('*');
						}
					}
				case 5:
					for(int k=1; k<7; k++) {
						System.out.print(' ');
						if(k==3) {
							System.out.print('*');
						}
					}
				case 6:
					for(int k=1; k<7; k++) {
						if(k==3) {
							System.out.print('*');
						}else {
							System.out.print(' ');
						}
					
				
			}
				
		}
		
		
		
		
	}

}
